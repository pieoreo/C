#include<stdio.h>

int main(void)
{
    string nicknames[4];

    nicknames[0] = "universe";
    nicknames[1] = "softtofu";
    nicknames[2] = "fixer";
    nicknames[3] = "pieoreo";

    printf("%s\n", nicknames[0]);
    printf("%c%c%c%c\n", nicknames[0][0], nicknames[0][1], nicknames[0][2], nicknames[0][3]);

}
